{
  "manifest_version": 2,
  "name": "HPUSRA",
  "author": "Ahmad Mohamad Shafiee",
  "description": "This extension make log file from visited urls.",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "log-icon.png",
    //    "default_popup": "popup.html",
    "default_title": "HPUSRA"
  },
  "permissions": [
    "tabs",
    "unlimitedStorage"
    // or run chrome with --unlimited-quota-for-files flag
    //    "activeTab",
    //    "https://ajax.googleapis.com/"
  ]
}